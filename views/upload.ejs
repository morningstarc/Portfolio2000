<!doctype html>
<html lang="en">


    <% include ./partials/header %>
    <link rel="stylesheet" href="../public/css/form.css">

    <title>User's Home</title>
</head>
 <% include ./partials/nav%>

  <body class="d-flex flex-column">
  <div id="page-content"> 
  <header class="masthead">
  <div class="container h-100">
    <div class="row h-100 align-items-center">
      
  <div class="col-md-3"></div>   
  <div class="col-md-6" style="margin-top:75px">
 <form action="/upload" method="POST">
            <div class="form-group">
                <label for="title">Title of Project</label>
                <input type="text" class="form-control" name="title" id="title" placeholder="Title of project">
            </div>
            <div class="form-group">
                <label for="type">Type of Project</label>
                <input type="text" class="form-control" name="type" id="type" placeholder="Type of project, ie. website">
            </div>
            <div class="form-group">
                <label for="link">Link</label>
                <input type="text" class="form-control" name="link" id="link" placeholder="Link">
            </div>
              <div class="form-group">
                <label for="github">Git Hub Link</label>
                <input type="text" class="form-control" name="github" id="github" placeholder="Github link">
            </div>
              <div class="form-group">
                <label for="code">Code</label>
                <input type="textarea" class="form-control" name="code" id="code" placeholder="Code">
            </div>
            <button type="submit" class="btn btn-primary">Upload Project</button>
        </form>

  </div>
</div>
</div>
</div>
</header>
  </div>
  <footer id="sticky-footer" class="py-4 bg-dark text-white-50">
    <div class="container text-center">
      <small>Team: Austin Cauley, MorningStar Cornell</small>
    </div>
  </footer>

    <% include ./partials/scripts %>
</body>


</html>